window "PicView"
{
  Style: wfsNone
  Options: wfoNoBorder
  Frame: (0,0) - (201,21)
  Title: "PicView"
  component "First" is "Command Button"
  {
    Frame: (0,0) - (41,21)
    Caption: "First"
    connect 
    {
      signalCmdButtonClicked -> PicView::First
    }
  }
  component "Prev" is "Command Button"
  {
    Frame: (40,0) - (81,21)
    Caption: "Prev"
    connect 
    {
      signalCmdButtonClicked -> PicView::Prev
    }
  }
  component "Next" is "Command Button"
  {
    Frame: (80,0) - (121,21)
    Caption: "Next"
    connect 
    {
      signalCmdButtonClicked -> PicView::Next
    }
  }
  component "Quit" is "Command Button"
  {
    Frame: (120,0) - (161,21)
    Caption: "Quit"
    connect 
    {
      signalCmdButtonClicked -> PicView::Quit
    }
  }
  component "Scale" is "Command Button"
  {
    Frame: (160,0) - (201,21)
    Caption: "Scale"
    connect 
    {
      signalCmdButtonClicked -> PicView::Scale
    }
  }
}