<?xml version="1.0" ?>
<Falagard>
  
    <!--
    ***************************************************
        ice/SettingComboBox
    ***************************************************
    -->
    <WidgetLook name="ice/SettingComboBox">
      <PropertyLinkDefinition name="Name" widget="__auto_name__" targetProperty="Text" />

      <PropertyLinkDefinition name="LabelTextColour" widget="__auto_name__" targetProperty="TextColours" />
      <Child type="ice/StaticText" nameSuffix="__auto_name__">
        <Area>
          <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
          <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
          <Dim type="Width" ><UnifiedDim scale="0.6" type="Width" /></Dim>
          <Dim type="Height" ><FontDim type="LineSpacing" padding="8" /></Dim>
        </Area>
        <Property name="FrameEnabled" value="False" />
        <Property name="BackgroundEnabled" value="False" />
      </Child>
      <Child type="ice/Combobox" nameSuffix="__auto_combobox__">
        <Area>
          <Dim type="LeftEdge" ><UnifiedDim scale="0.6" offset="2" type="LeftEdge" /></Dim>
          <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
          <Dim type="Width" ><UnifiedDim scale="0.4" offset="-2" type="Width" /></Dim>
          <Dim type="BottomEdge" ><UnifiedDim scale="1" offset="40" type="BottomEdge" /></Dim>
        </Area>
        <Property name="ClippedByParent" value="False" />
      </Child>
      <ImagerySection name="normal">
      </ImagerySection>
      <StateImagery name="Enabled">
        <Layer>
          <Section section="normal" />
        </Layer>
      </StateImagery>
      <StateImagery name="Disabled">
        <Layer>
          <Section section="normal">
            <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
          </Section>
        </Layer>
      </StateImagery>
    </WidgetLook>
  
    <!--
    ***************************************************
        ice/SettingSlider
    ***************************************************
    -->
    <WidgetLook name="ice/SettingSlider">
      <PropertyLinkDefinition name="Name" widget="__auto_name__" targetProperty="Text" />
      <PropertyLinkDefinition name="MaximumValue" widget="__auto_slider__" targetProperty="MaximumValue" />
      <PropertyLinkDefinition name="ClickStepSize" widget="__auto_slider__" targetProperty="ClickStepSize" />

      <PropertyLinkDefinition name="LabelTextColour" widget="__auto_name__" targetProperty="TextColours" />
      <PropertyLinkDefinition name="ValueTextColour" widget="__auto_value__" targetProperty="TextColours" />
      <Child type="ice/StaticText" nameSuffix="__auto_name__">
        <Area>
          <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
          <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
          <Dim type="Width" ><UnifiedDim scale="0.4" type="Width" /></Dim>
          <Dim type="Height" ><FontDim type="LineSpacing" padding="8" /></Dim>
        </Area>
        <Property name="FrameEnabled" value="False" />
        <Property name="BackgroundEnabled" value="False" />
        <Property name="TextColours" value="tl:FFFFFFFF tr:FFFFFFFF bl:FFFFFFFF br:FFFFFFFF" />
      </Child>
      <Child type="ice/HorzSlider" nameSuffix="__auto_slider__">
        <Area>
          <Dim type="LeftEdge" ><UnifiedDim scale="0.4" offset="2" type="LeftEdge" /></Dim>
          <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
          <Dim type="Width" ><UnifiedDim scale="0.6" offset="-50" type="Width" /></Dim>
          <Dim type="Height" ><FontDim type="LineSpacing" padding="8" /></Dim>
        </Area>
      </Child>
      <Child type="ice/StaticText" nameSuffix="__auto_value__">
        <Area>
          <Dim type="LeftEdge" ><UnifiedDim scale="1" offset="-48" type="LeftEdge" /></Dim>
          <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
          <Dim type="Width" ><AbsoluteDim value="48" /></Dim>
          <Dim type="Height" ><FontDim type="LineSpacing" padding="8" /></Dim>
        </Area>
        <Property name="FrameEnabled" value="False" />
        <Property name="BackgroundEnabled" value="False" />
      </Child>
      <ImagerySection name="normal">
      </ImagerySection>
      <StateImagery name="Enabled">
        <Layer>
          <Section section="normal" />
        </Layer>
      </StateImagery>
      <StateImagery name="Disabled">
        <Layer>
          <Section section="normal">
            <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
          </Section>
        </Layer>
      </StateImagery>
    </WidgetLook>
  
  
