                <Image type="BottomEdge" imageset="ice" image="VertScrollBottom" />
                <Image type="Background" imageset="ice" image="VertScrollMiddle" />
            </FrameComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><UnifiedDim scale="0.45" type="LeftEdge" /></Dim>
                    <Dim type="TopEdge" >
                        <WidgetDim widget="__auto_decbtn__" dimension="Height">
                            <DimOperator op="Add">
                                <ImageDim imageset="ice" image="VertScrollTop" dimension="Height">
