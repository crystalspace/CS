                    <Dim type="BottomEdge" >
                        <UnifiedDim scale="1.0" type="BottomEdge">
                            <DimOperator op="Subtract">
                                <WidgetDim widget="__auto_decbtn__" dimension="Height">
                                    <DimOperator op="Add">
                                        <ImageDim imageset="ice" image="VertScrollTop" dimension="Height">
                                            <DimOperator op="Multiply">
                                                <AbsoluteDim value="0.5" />
                                            </DimOperator>
                                        </ImageDim>
                                    </DimOperator>
                                </WidgetDim>
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
                </Area>
                <Image imageset="ice" image="VertScrollBarSegment" />
                <VertFormat type="Tiled" />
            </ImageryComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="main" />
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="main">
                    <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>

