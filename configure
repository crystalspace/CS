fi


    if test "$cs_prog_path_prepared" != yes; then :
  cs_prog_path_prepared=yes
	PATH="$PATH$PATH_SEPARATOR/usr/local/bin$PATH_SEPARATOR/sw/bin$PATH_SEPARATOR${srcdir}/bin"
	export PATH
fi
    for ac_prog in install
