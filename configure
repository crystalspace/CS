# Result Summary.
#------------------------------------------------------------------------------
cs_summary=`echo "$_cs_summary" | sort -i -t '|' -k 3,3r -k 1,1 |
    awk '
	BEGIN {
          FS="|";
          heading["yes"]="Found";
          heading["no"]="Missing";
          ok=""
        }
	!/^$/ {
	  if (ok != $3) {
            ok=$3;
            if (NR != 1)
              print "";
            print heading[ok]
          }
	  if ($3 != "no" || ($4 == "*" || $4 == "'"$cs_host_family"'"))
	  printf "  %-20s (%s)\n", $1, $2
	}'
    `
{ $as_echo "$as_me:${as_lineno-$LINENO}:
******************************************************************************
***                 Crystal Space -- Configuration Summary                 ***
******************************************************************************
Build mode: ${cs_build_mode}
Version: ${PACKAGE_VERSION} (SVN: ${cs_cv_source_is_svn})

Optional Dependencies
---------------------
$cs_summary" >&5
$as_echo "$as_me:
******************************************************************************
***                 Crystal Space -- Configuration Summary                 ***
******************************************************************************
Build mode: ${cs_build_mode}
Version: ${PACKAGE_VERSION} (SVN: ${cs_cv_source_is_svn})

Optional Dependencies
---------------------
$cs_summary" >&6;}


